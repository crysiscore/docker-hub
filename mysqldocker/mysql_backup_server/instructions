
###########################   MYSQL_SERVER     ############# ##############
docker pull crysiscore/mysql_master:v5.6  
 docker run --name mysql_server -v /data_ssd_1/mysql_conf:/etc/mysql/conf.d  -v /data_ssd_1/mysql_data_dir:/data  -p 3306:3306  -e MYSQL_ROOT_PASSWORD=password  -d  mysql:5.6.49

     docker run -d \
    --name mysql_server_sync \
    -p 5457:3306 \
    -e MYSQL_USER=esaude \
    -e MYSQL_PASSWORD=esaude \
	-e MYSQL_DATABASE=openmrs \
	-e MYSQL_ROOT_PASSWORD=password  \
	-v /data_ssd_1:/data \
     crysiscore/mysql_master:v5.6 


     docker run -d \
    --name mysql_server_docker_openmrs \
    -p 3306:3306 \
    -e MYSQL_USER=esaude \
    -e MYSQL_PASSWORD=esaude \
	-e MYSQL_DATABASE=openmrs \
	-e MYSQL_ROOT_PASSWORD=password  \
	-v /data_ssd_2:/data \
     crysiscore/mysql_master:v5.6 

